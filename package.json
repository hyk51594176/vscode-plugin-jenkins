{
	"name": "vscode-plugin-jenkins",
	"displayName": "",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "jenkins",
					"title": "jenkins",
					"icon": "images/jenkins.svg"
				}
			]
		},
		"views": {
			"jenkins": [
				{
					"id": "jenkinsExplorer",
					"name": "jenkins"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "jenkinsExplorer.refreshEntry",
					"when": "view == jenkinsExplorer",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
				"command": "jenkinsExplorer.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "images/refresh_dark.svg",
					"dark": "images/refresh_light.svg"
				}
			},
			{
				"command": "jenkinsExplorer.publish",
				"title": "jenkins publish"
			}
		],
		"configuration": {
			"type": "object",
			"title": "jenkins configuration",
			"properties": {
				"jenkins.url": {
					"type": "string",
					"description": "jenkins地址"
				},
				"jenkins.name": {
					"type": "string",
					"description": "用户名"
				},
				"jenkins.password": {
					"type": "string",
					"description": "密码"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"jenkins": "^0.23.0"
	}
}
